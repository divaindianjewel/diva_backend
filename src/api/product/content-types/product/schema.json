{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal"
    },
    "compare_price": {
      "type": "decimal"
    },
    "stock": {
      "type": "decimal"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "feature": {
      "type": "boolean"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "both",
        "male",
        "female"
      ]
    },
    "color": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::color.color"
    },
    "desctiption": {
      "type": "richtext",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "youtube_link": {
      "type": "string"
    }
  }
}
